{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:GBDK_HOME}/include/**",
                "${env:GBDK_HOME}/libc_src/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "__TARGET_gb",
                "__sfr=/*sfr*/",
                "__banked=/*banked*/",
                "__naked=/*naked*/",
                "start_comment=/",
                "__asm=start_comment ## *",
                "__endasm=*/",
                "__PORT_sm83",
                "__SDCC_sm83",
                "__SDCC=4_5_1",
                "__SDCC_REVISION=99999"
            ],
            "windowsSdkVersion": "10.0.26100.0",
            "compilerPath": "C:\\tools\\gbdk\\bin\\sdcc.exe",
            "cStandard": "c23",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}